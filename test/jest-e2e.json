{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": "../",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@dlabs/common/(.*)": "<rootDir>/libs/common/src/$1",
    "@dlabs/common": "<rootDir>/libs/common/src",
    "@dlabs/test-starter/(.*)": "<rootDir>/libs/test-starter/src/$1",
    "@dlabs/test-starter": "<rootDir>/libs/test-starter/src"
  }
}
