{
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "projects": {
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    },
    "security": {
      "type": "library",
      "root": "libs/security",
      "entryFile": "index",
      "sourceRoot": "libs/security/src",
      "compilerOptions": {
        "tsConfigPath": "libs/security/tsconfig.lib.json"
      }
    },
    "test-starter": {
      "type": "library",
      "root": "libs/test-starter",
      "entryFile": "index",
      "sourceRoot": "libs/test-starter/src",
      "compilerOptions": {
        "tsConfigPath": "libs/test-starter/tsconfig.lib.json"
      }
    }
  },
  "compilerOptions": {
    "webpack": false,
    "plugins": [
      {
        "name": "@nestjs/swagger/plugin",
        "options": {
          "dtoFileNameSuffix": [
            ".request.ts",
            ".response.ts",
            ".param.ts"
          ]
        }
      }
    ]
  }
}